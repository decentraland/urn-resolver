{
  "name": "@dcl/urn-resolver",
  "version": "1.0.0",
  "description": "URN resolver for Decentraland assets",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "make update-contracts",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "export TS_NODE_PROJECT='./test/tsconfig.json'; node --require ts-node/register --async-stack-traces --experimental-modules node_modules/.bin/_mocha --timeout 10000 --reporter spec",
    "lint:check": "eslint '**/*.{js,ts}'",
    "lint:fix": "eslint '**/*.{js,ts}' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/urn-resolver.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/decentraland/urn-resolver/issues"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 2
  },
  "homepage": "https://github.com/decentraland/urn-resolver#readme",
  "devDependencies": {
    "@dcl/eslint-config": "^2.0.0",
    "@microsoft/api-extractor": "^7.18.4",
    "@types/mocha": "^10.0.1",
    "expect": "^29.0.1",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.3.5",
    "urn-lib": "^2.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
