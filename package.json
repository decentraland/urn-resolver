{
  "name": "@dcl/urn-resolver",
  "version": "1.0.0",
  "description": "URN resolver for Decentraland assets",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "./update-contracts.sh",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "jest --forceExit --detectOpenHandles --coverage --verbose",
    "test-old": "export TS_NODE_PROJECT='./test/tsconfig.json'; node --require ts-node/register --async-stack-traces --experimental-modules node_modules/.bin/_mocha --timeout 10000 --reporter spec",
    "lint:check": "eslint '**/*.{js,ts}'",
    "lint:fix": "eslint '**/*.{js,ts}' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/urn-resolver.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/decentraland/urn-resolver/issues"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 2
  },
  "homepage": "https://github.com/decentraland/urn-resolver#readme",
  "devDependencies": {
    "@dcl/eslint-config": "^2.0.0",
    "@microsoft/api-extractor": "^7.18.4",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "urn-lib": "^2.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
